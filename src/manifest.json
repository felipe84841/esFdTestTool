{
  "description": "Testing Tool changes the user Agent",
  "manifest_version": 2,
  "name": "esFdTestTool",
  "version": "0.1",
  "homepage_url": "https://felipe84841.github.io/esFdTestTool/",
  "author": "Felipe Desiglo",
  "icons": {
    "32": "icons/target-32.png",
    "48": "icons/target-48.png"
  },
  
    "permissions": [
		"webRequest",
		"webRequestBlocking",		
		"*://*/*"
   ],
   
    "background": {
		"scripts": ["background.js"]
	},
	
	"browser_action": {
		"default_icon": {
          "16": "icons/target-16.png",
          "32": "icons/target-32.png"
        },
		"default_title": "Choose a user agent",
		"default_popup": "page/page.html"
   }

}